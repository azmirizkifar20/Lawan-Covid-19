        <activity android:name="org.marproject.lawancovid19.ui.produktif.ProduktifActivity" />
        <activity android:name="org.marproject.lawancovid19.ui.global.DiagnosaMandiriActivity" />
        <activity android:name="org.marproject.lawancovid19.ui.global.StatistikInterActivity" />
        <activity android:name="org.marproject.lawancovid19.ui.global.GlobalActivity" />
        <activity android:name="org.marproject.lawancovid19.ui.provinsi.ProvinsiActivity" />
        <activity android:name="org.marproject.lawancovid19.ui.menu.MenuActivity" />
        <activity android:name="org.marproject.lawancovid19.ui.splash.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>