        android:allowBackup="true"
        android:icon="@mipmap/lawan_covid_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="org.marproject.lawancovid19.ui.produktif.BeritaTerkiniActivity"/>
        <activity android:name="org.marproject.lawancovid19.ui.menu.GrafikActivity" />
        <activity android:name="org.marproject.lawancovid19.ui.produktif.BacaQuranActivity" />
